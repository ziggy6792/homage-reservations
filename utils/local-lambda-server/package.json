{
  "name": "@alpaca-backend/local-lambda-server",
  "version": "2.0.0",
  "description": "Lambda function generated by Amplify",
  "main": "dist/index.js",
  "license": "Apache-2.0",
  "scripts": {
    "start": "ts-node-dev -r tsconfig-paths/register src/index.ts --watch --respawn src/index.ts",
