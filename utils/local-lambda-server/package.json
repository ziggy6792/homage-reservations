    "@alpaca-backend/lambda-gq-resolver": "^2.0.0",
    "@alpaca-backend/lambda-user-confirmed": "^2.0.0",
    "aws-sdk": "2.901.0",
    "express": "^4.17.1",
    "lambda-local": "^1.7.4",
    "lodash": "^4.17.21",
    "source-map-support": "^0.5.19",
    "ts-node-dev": "^1.1.8"
  }
}
